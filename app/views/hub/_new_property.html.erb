<h1> Create New Property </h1>

<div class="row">
  <div class="col-xs-10 col-xs-offset-1">
    <%= bootstrap_form_for(@property, url: { controller: "hub", action: "create_property" }, method: :post, remote: true) do |f| %>
      <%= render 'properties/fields', f: f %>
      <%= hidden_field_tag :selected_entity, "" %>
      <%= hidden_field_tag :selected_group, "" %>
      <%= f.submit "Create Property", class: "btn btn-sm btn-primary btn110", id: "create-property" %>
    <% end %>
  </div>
</div>