$("#debug").html('<%= params %>');

<% unless params[:search] == "entity" || params[:event] == "entity" %>
    <% unless params[:search] == "property" || params[:event] == "property" %>
        $('#groups').html("<%= j render partial: 'groups/groups', locals: { show_created: false, show_remove: false } %>");
    <% end %>
<% end %>

<% unless params[:search] == "group" || params[:event] == "group" %>
    <% unless params[:search] == "property" || params[:event] == "property" %>
        <% unless params[:event] == "entitys_groups" %>
            $('#entities').html("<%= j render partial: 'entities/entities', locals: { show_created: false, show_remove: false } %>");
        <% end %>
    <% end %>
<% end %>

<% unless params[:search] == "group" || params[:event] == "group" %>
    <% unless params[:search] == "entity" || params[:event] == "entity" %>
        <% unless params[:event] == "entitys_groups" %>
            $('#properties').html("<%= j render partial: 'properties/properties', locals: { show_units: false, show_created: false, show_remove: false } %>");
        <% end %>
    <% end %>
<% end %>

persistStyling();