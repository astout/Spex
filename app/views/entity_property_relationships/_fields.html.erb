<%= f.text_field :entity, disabled: true, class: "input-sm", value: relation.entity.name || "" %>

<%= f.text_field :group, disabled: true, class: "input-sm", value: relation.group.name || "" %>

<%= f.text_field :property, disabled: true, class: "input-sm", value: relation.property.name || "" %>

<%= f.number_field :order, disabled: true, class: "input-sm", value: relation.order+1 || ""%>

<%= f.text_field :label, class: "input-sm", value: relation.label || "" %>

<%= f.text_field :value, class: "input-sm", value: relation.value || "" %>

<%= f.number_field :visibility, min: 0, class: "input-sm", value: relation.visibility || "" %>
