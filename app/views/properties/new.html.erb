<% provide(:title, 'New Property') %>
<h1>New Property</h1>

<div class="row">
  <div class="col-xs-6 col-xs-offset-3">
    <%= bootstrap_form_for(@property, inline_errors: false) do |f| %>
      <%= render 'properties/fields', f: f %>
      <%= f.submit "Create Property", class: "btn btn-large btn-primary btn150"%>
    <% end %>
  </div>
</div>