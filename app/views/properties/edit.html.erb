<% provide(:title, "Edit property") %>
<h1>Update property profile</h1>

<div class="row">
  <div class="col-xs-6 col-xs-offset-3">
    <%= bootstrap_form_for(@property, inline_errors: false) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary btn150" %>
    <% end %>
  </div>
</div>