<% provide(:title, "Edit property") %>
<h1>Update Property Definition</h1>
<div class="row">
  <div class="col-xs-6 col-xs-offset-3">
    <%= bootstrap_form_for(@property, inline_errors: false) do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary btn150" %>
    <% end %>
  </div>
</div>
<div class="row">
    <hr/>
    <h4>Add Properties</h4>
    <span class="col-xs-10 col-xs-offset-1">
    </span>
</div>