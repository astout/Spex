<% provide(:title, 'New Group') %>
<h1>New Group</h1>

<div class="row">
  <div class="col-xs-6 col-xs-offset-3">
    <%= bootstrap_form_for(@group, inline_errors: false) do |f| %>
      <%= render 'groups/fields', f: f %>
      <%= f.submit "Create Group", class: "btn btn-large btn-primary btn150"%>
    <% end %>
  </div>
</div>