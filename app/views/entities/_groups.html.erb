<% if defined? @egrs %>
    <% unless @egrs[:data].empty? %>
        <script type="text/javascript">
            console.log("doing it")
        </script>
        <%= render partial: 'entities/group_relation_actions' %>
        <table class="table table-condensed egrs">
            <th>
                Name
            </th>
            <th>
                Label
            </th>
            <% if show_created %>
                Created
            <% end %>
            <th>
                Position
            </th>
            <% if show_remove %>
                <th>
                    Remove
                </th>
            <% end %>
            <%= render partial: 'entities/group_relationship', collection: @egrs[:data], locals: { show_created: show_created, show_remove: show_remove }  %>
        </table>
        <%= will_paginate @egrs[:data], inner_window: 0, outer_window: 0, param_name: 'egrs_page', params: { entitys_group_event: true } %>
    <% else %>
        <script type="text/javascript">
            console.log("no data")
        </script>
    <% end %>
<% else %>
    <div class='alert alert-info small-font center'>
        <i>No entity selected.</i>
    </div>
<% end %>
