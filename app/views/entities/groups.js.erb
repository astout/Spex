$('input[id^=entity]').val('');

function flash_alert() {
    var result = "<% unless @result.nil? %>";
        result+= "<% if @result[:r] == 1 %>";
        result+= "    <div class='alert alert-success'>";
        result+= "    <%= @result[:msg] %>";
        result+= "    </div>";
        result+= "<% else %>";
        result+= "    <div class='alert alert-danger'>";
        result+= "        <%= @result[:msg] %>";
        result+= "    </div>";
        result+= "<% end %>";

    result+= "<% else %>";
    result+= "    <div class='alert alert-danger'>";
    result+= "        There was an error retrieving #{@fetched_entity.name}'s groups";
    result+= "    </div>";
    result+= "<% end %>";

    return result;
}