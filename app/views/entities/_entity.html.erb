<% unless entity.properties.blank? && !admin_user? %>
  <tr id=<%= "#{entity.id}" %> class="list-item entity">
      <td>
        <%= link_to entity.name, entity %>
      </td>
      <td>
        <%= link_to entity.label || "", entity %>
      </td>
      <% if show_created %>
        <td>
          <%= "#{entity.created_at.strftime('%m/%d/%Y')}" %>
        </td>
      <% end %>
      <td>
        <%= "#{entity.updated_at.strftime('%m/%d/%Y %H:%M')}" %>
      </td>
      <% if admin_user? && show_remove %>
        <td class="delete-entity">
          <span id="<%= entity.id %>"  class="glyphicon glyphicon-trash delete alert-danger">
          </span>
        </td>
      <% end %>
  </tr>
<% end %>
